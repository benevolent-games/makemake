		"start": "http-server",
		"test": "cynic node x/tests.test.js"
	},
	"keywords": [
		"3d",
		"engine",
		"games"
	],
	"dependencies": {
		"@babylonjs/core": "^5.13.3",
		"es-module-shims": "^1.5.5"
	},
	"devDependencies": {
		"cynic": "0.1.0-dev.16",
		"http-server": "^14.1.0",
		"importly": "^0.2.0",
		"mkdirp": "^1.0.4",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"typescript": "^4.7.2"
	}
}
