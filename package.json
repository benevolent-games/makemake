{
	"name": "@chasemoskal/makemake",
	"description": "3d game library",
	"version": "0.0.0-dev.0",
	"license": "MIT",
	"type": "module",
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"files": [
		"x",
		"s"
	],
	"main": "x/makemake.js",
	"scripts": {
		"build": "run-s clean && run-p compile importly",
		"clean": "rimraf x && mkdirp x",
